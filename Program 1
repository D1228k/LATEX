PROGRAM 11

\documentclass{article}
\usepackage{algorithm}
\usepackage{algorithmic}
\begin{document}
	\begin{algorithm}
		\caption{Find Maximum Value}
		\begin{algorithmic}[1]
			\REQUIRE $A$ is a list of $n$ numbers
			\ENSURE $max\_value$ is the maximum number in the list
			\STATE $max\_value \leftarrow A[1]$
			\FOR{$i \leftarrow 2$ to $n$}
			\IF{$A[i] > max\_value$}
			\STATE $max\_value \leftarrow A[i]$
			\ENDIF
			\ENDFOR
			\RETURN $max\_value$
		\end{algorithmic}
	\end{algorithm}
\end{document}

