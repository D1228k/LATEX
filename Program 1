PROGRAM 10 

\documentclass{article}
\usepackage{tikz}
\usetikzlibrary{trees}

\begin{document}
	
	\title{Organization Hierarchy Diagram}

	
	\section{Hierarchy Diagram}
	
	\begin{center}
		\begin{tikzpicture}[
			level 1/.style={sibling distance=40mm},
			level 2/.style={sibling distance=20mm},
			edge from parent/.style={->,draw},
			>=latex]
			
			% Root node
			\node {Organization}
			% Level 2 nodes
			child {node {Department A}
				child {node {Team 1}}
				child {node {Team 2}}
				child {node {Team 3}}
			}
			child {node {Department B}
				child {node {Team 4}}
				child {node {Team 5}}
			}
			child {node {Department C}
				child {node {Team 6}}
				child {node {Team 7}}
				child {node {Team 8}}
			};
		\end{tikzpicture}
	\end{center}
	
\end{document}

